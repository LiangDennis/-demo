/* pages/navigate/navigate.wxss */
.container-navigate {
  background-color: black;
  color: white;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}
.header {
  color: pink;
  font-size: 48rpx;
  text-align: center;
  width: 90%;
  height: 70rpx;
  background-color: red;
  border-radius: 10rpx;
  margin: 20rpx auto;
}
.content {
  width: 90%;
  display: flex;
  flex-direction: column;
  background-color: blue;
  margin: 0 auto;
}
.content .navbar {
  flex: 1;
}
.content .navbar button {
  margin: 20rpx 0;
}